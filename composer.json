{
    "name": "xgrz/settings",
    "description": "Easy handling for app settings in laravel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "settings"
    ],
    "autoload": {
        "psr-4": {
            "xGrz\\Settings\\": "src/",
            "xGrz\\Settings\\Database\\Factories\\": "database/factories/"
        },
        "files": [
            "src/Helpers/global.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "xGrz\\Settings\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "xGrz\\Settings\\SettingsServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Grzegorz By≈õkiniewicz",
            "email": "grzesiek.byskiniewicz@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0"
    }
}
